{
  "resourceType" : "Observation",
  "id": "<%- id %>",
  "subject" : {
    "reference": "urn:uuid:<%- subject.id %>"<% if (subject.name) { %>,
    "display": "<%- subject.name %>"
    <% } %>
  },
  
  "status" : "<%- (status)? status : 'final' %>"

  <%- include("/helpers/codableConcept", {"name" : "code", "obj" : code}) %>
  <%- include("/helpers/codableConcept", {"name" : "category", "obj" : [
        {
        "system": "http://terminology.hl7.org/CodeSystem/observation-category",
          "code": category
        } ]
      }) %>


<% if( condition ) { %>
  , "focus" : [
    {
      "reference": "urn:uuid:<%- condition.id %>"<% if (condition.name) { %>,
      "display": "<%- condition.name %>"
      <% } %>
    }
  ]
  <% } %>


  <%- include("/helpers/effectiveX", {"effective" : effectiveDate}) %>
  <%- include("/helpers/valueX", {"value" : value}) %>
  <%- include("/helpers/bodySite", {"bodySite" : bodySite, "laterality": laterality}) %>


}